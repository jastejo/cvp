    "import numpy as np\n",
    "import scipy.integrate as integrate\n",
    "import matplotlib.pyplot as plt\n",
    "f=lambda x:x**2+3\n",
    "x=np.linspace(-1,2,200)\n",
    "fx=f(x)\n",
    "gx=np.ones(fx.shape)\n",
    "plt.plot(x,fx,gx)\n",
    "plt.fill_between(x,fx,gx)\n",
    "plt.xlim((-3,3))\n",
    "plt.title(\"Area between two curves\")\n",
    "plt.show()"
   ]
